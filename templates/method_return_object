[% ret = method.return -%]
package [% ret.name %];
  use Moose;

  [%- FOREACH property = ret.attributes %]
  has [% property.name %] => (is => 'ro'
    [%- IF (property.required) %], required => 1[% END -%]
    [%- IF (property.perl_type) %], isa => '[% property.perl_type %]'[% END -%]
  );
  [%- END %]

1;
